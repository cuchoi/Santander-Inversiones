{
  "manifest_version": 2,

  "name": "Santander Inversiones",
  "description": "Esta extensi√≥n te ayuda a manejar tus extensiones de Santander.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.santander.cl/*",
    "http://www.santander.cl/*",
    "activeTab",
    "tabs",
    "storage"

  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}